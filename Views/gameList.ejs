<div>
    <% data.forEach((game) => { %>
        <div class="card">
            <p><%=game.title%></p>
            <p><%=game.description%></p>
            <p><%=game.price%></p>
            <img src=<%=game.image%>>
            <p><%=game.publisher%></p>
            <p><%=game.reviews%></p>
            <p><%=game.genre%></p>
        </div>
    <%})%>
</div>
